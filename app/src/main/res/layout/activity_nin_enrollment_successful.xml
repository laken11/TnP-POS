<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/enrollment_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="@drawable/enrollment"
    tools:targetApi="s">

    <RelativeLayout
        android:id="@+id/frame_28"
        android:layout_width="170dp"
        android:layout_height="170dp"
        android:layout_above="@+id/enrollment_success_text"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="123dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/enrollment"
        android:clipToOutline="true">

        <ImageView
            android:id="@+id/dashed_border"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_centerInParent="true"
            android:contentDescription=""
            android:src="@drawable/dashed_border" />

        <ImageView
            android:id="@+id/circle_with_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/circle_with_check" />

    </RelativeLayout>

    <TextView
        android:id="@+id/enrollment_success_text"
        android:textAlignment="center"
        android:layout_width="337dp"
        android:layout_height="44dp"
        android:layout_above="@+id/enrollment_congrats"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="12dp"
        android:lineSpacingExtra="3sp"
        android:text="@string/enrollment_success_text"
        android:textAppearance="@style/enrollment_success_text" />

    <TextView
        android:id="@+id/enrollment_congrats"
        android:layout_width="341dp"
        android:layout_height="48dp"
        android:layout_above="@+id/frame_22"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="42dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="18dp"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="2sp"
        android:text="@string/enrollment_congrats"
        android:textAppearance="@style/enrollment_congrats" />

    <RelativeLayout
        android:id="@+id/frame_22"
        android:layout_width="284dp"
        android:layout_height="58dp"
        android:layout_above="@+id/payment_det"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="66dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/frame_enrollment_number">

        <TextView
            android:id="@+id/enrollment_number"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="13dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="45dp"
            android:layout_marginBottom="14dp"
            android:gravity="center_vertical"
            android:lineSpacingExtra="2sp"
            android:text=""
            android:textAppearance="@style/enrollment_number" />

        <ImageButton
            android:id="@+id/copy_2"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="14dp"
            android:layout_marginBottom="18dp"
            android:contentDescription="TODO"
            android:src="@drawable/copy" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/payment_det"
        android:layout_width="350dp"
        android:layout_height="145dp"
        android:layout_above="@+id/back_to_home"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/enrollment_details"
        android:elevation="24dp">

        <RelativeLayout
            android:id="@+id/details_row"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="20dp">

            <TextView
                android:id="@+id/account_name"
                android:layout_width="196dp"
                android:layout_height="18dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="11dp"
                android:layout_marginBottom="47dp"
                android:gravity="top"
                android:lineSpacingExtra="0sp"
                android:text="@string/account_name"
                android:textAppearance="@style/account_name"
                android:translationY="-0.15sp" />

            <TextView
                android:id="@+id/account_number"
                android:layout_width="199dp"
                android:layout_height="18dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="11dp"
                android:layout_marginEnd="140dp"
                android:layout_marginBottom="16dp"
                android:gravity="top"
                android:lineSpacingExtra="0sp"
                android:text="@string/account_number"
                android:textAppearance="@style/account_number"
                android:translationY="-0.15sp" />

            <TextView
                android:id="@+id/account_number_text"
                android:layout_width="75dp"
                android:layout_height="18dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="42dp"
                android:layout_marginBottom="15dp"
                android:gravity="center_horizontal|top"
                android:lineSpacingExtra="0sp"
                android:textAlignment="center"
                android:textAppearance="@style/account_number_text"
                android:translationY="-0.15sp" />

            <ImageButton
                android:id="@+id/copy"
                android:layout_width="21dp"
                android:layout_height="23dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="12dp"
                android:contentDescription="TODO"
                android:src="@drawable/copy" />

            <TextView
                android:id="@+id/account_name_text"
                android:layout_width="102dp"
                android:layout_height="18dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="74dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="47dp"
                android:gravity="center_horizontal|top"
                android:lineSpacingExtra="0sp"
                android:textAlignment="center"
                android:textAppearance="@style/account_name_text"
                android:translationY="-0.15sp" />

            <TextView
                android:id="@+id/virtual_acc"
                android:layout_width="144dp"
                android:layout_height="18dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="102dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="104dp"
                android:layout_marginBottom="103dp"
                android:lineSpacingExtra="0sp"
                android:text="@string/virtual_acc"
                android:textAlignment="center"
                android:textAppearance="@style/virtual_acc"
                android:translationY="-0.15sp" />
        </RelativeLayout>


    </RelativeLayout>

    <Button
        android:id="@+id/back_to_home"
        android:layout_width="350dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="23dp"
        android:backgroundTint="@color/red"
        android:gravity="center"
        android:text="@string/go_to_home"
        android:textAppearance="@style/proceed"
        android:textSize="18sp" />

</RelativeLayout>